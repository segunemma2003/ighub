	<script type="text/javascript">
		
		// var y = [1,3,5,7,9,11,13,15,17];
		// var x = 13;
		// for(let i = 0; i<y.length; i++){
		// 	if(x == y[i]) {
		// 	console.log(i);
   //       }else{
  //       console.log("not found");
        
		// }}
       var y = [1,3,5,7,9,11,13,15,17];
      var x = 13;
      let left = 0;
      let right = y.length - 1;
    while (left <= right) {
        const mid = left + Math.floor((right - left) / 2);
        if (y[mid] === x) {
            console.log(mid);
        }
        if (y[mid] < x) {
            left = mid + 1;
        } else {
            right = mid - 1;
        }
    }
    console.log("not found");
     
	</script>
